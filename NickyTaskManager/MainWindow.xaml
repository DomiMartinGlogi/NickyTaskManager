<Window x:Class="NickyTaskManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NickyTaskManager"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="ListSelect" Initialized="ListSelect_OnInitialized" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" VerticalAlignment="Top" Width="180"/>
        <ComboBox x:Name="ItemSelect" HorizontalAlignment="Left" Margin="208,10,0,0" VerticalAlignment="Top" Width="182" Height="23"/>
        <Button x:Name="CreateList" Content="Create List" HorizontalAlignment="Left" Margin="10,97,0,0" VerticalAlignment="Top" Width="180" Height="20"/>
        <TextBox HorizontalAlignment="Left" Margin="10,72,0,0" TextWrapping="Wrap" Text="New List" VerticalAlignment="Top" Width="180" Height="20"/>
    </Grid>
</Window>
